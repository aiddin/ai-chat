{
  "name": "@a.izzuddin/ai-chat",
  "version": "0.2.10",
  "description": "A framework-agnostic AI chat web component. Works with React, Vue, Svelte, Angular, and vanilla JavaScript.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "custom-elements.json",
    "global.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "customElements": "custom-elements.json",
  "keywords": [
    "ai",
    "chat",
    "chatbot",
    "web-components",
    "custom-elements",
    "lit",
    "typescript",
    "framework-agnostic",
    "react",
    "vue",
    "svelte",
    "angular"
  ],
  "author": "a.izzuddin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aiddin/ai-chat.git"
  },
  "bugs": {
    "url": "https://github.com/aiddin/ai-chat/issues"
  },
  "homepage": "https://github.com/aiddin/ai-chat#readme",
  "scripts": {
    "dev": "next dev",
    "dev:demo": "next dev",
    "build": "tsup && npm run analyze",
    "analyze": "custom-elements-manifest analyze --litelement",
    "build:demo": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "prepublishOnly": "npm run build",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build"
  },
  "dependencies": {
    "lit": "^3.3.1"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.11.0",
    "@playwright/test": "^1.57.0",
    "@shadcn/ui": "^0.0.4",
    "@tailwindcss/postcss": "^4",
    "@tauri-apps/cli": "^2.9.4",
    "@types/node": "^20",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "eslint": "^9",
    "eslint-config-next": "16.0.5",
    "markdown-it": "^14.1.0",
    "next": "16.0.5",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwindcss": "^4.1.17",
    "tsup": "^8.5.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }

}
